{
  "name": "@edx/frontend-component-site-header",
  "version": "2.0.0",
  "description": "A reusable React site header.",
  "repository": {
    "type": "git",
    "url": "https://github.com/edx/frontend-component-site-header.git"
  },
  "main": "dist/main.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --mode production --display-modules",
    "build-example": "webpack --config ./webpack.dev.config.js --mode production",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "webpack-dev-server --config ./webpack.dev.config.js --open --mode development",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "test": "jest --coverage",
    "snapshot": "jest --updateSnapshot",
    "semantic-release": "semantic-release"
  },
  "license": "GNU",
  "dependencies": {
    "react-responsive": "^6.1.1",
    "react-transition-group": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^6.0.2",
    "@commitlint/prompt": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "@edx/edx-bootstrap": "^2.2.1",
    "@edx/frontend-i18n": "^1.0.4",
    "@edx/frontend-logging": "^2.0.3",
    "@svgr/webpack": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es-check": "^5.0.0",
    "eslint-config-edx": "^4.0.4",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.6.0",
    "sass-loader": "^7.1.0",
    "semantic-release": "^17.2.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "@edx/frontend-i18n": "^1.0.4",
    "@edx/frontend-logging": "^2.0.3",
    "@edx/paragon": "^3.8.0",
    "prop-types": "^15.5.10",
    "react": "^16.4.2",
    "react-dom": "^16.2.0",
    "react-intl": "^2.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./src/tests/setupTest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/tests/setupTest.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!*.test.*"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgrMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
